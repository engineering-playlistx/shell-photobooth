{
  "name": "photobooth",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "fe": "pnpm --filter photobooth-frontend",
    "wb": "pnpm --filter photobooth-web",
    "dev": "concurrently \"pnpm fe dev\" \"pnpm wb dev\"",
    "lint": "pnpm fe lint && pnpm wb lint",
    "lint:fix": "pnpm fe lint:fix && pnpm wb lint:fix",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=24.10"
  },
  "packageManager": "pnpm@10.18.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "esbuild",
      "sharp",
      "supabase",
      "unrs-resolver",
      "workerd"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@types/lodash-es": "^4.17.12",
    "concurrently": "^9.2.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "lodash-es": "^4.17.21",
    "tailwind-merge": "^3.3.1",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,css,md}": [
      "sh -c 'pnpm lint:fix'"
    ]
  }
}
